# 1. 两数之和

class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        vector<int> ret;
        unordered_map<int, int> mp;
        for(int i = 0; i < nums.size(); i++){
            if(mp.find(target - nums[i]) != mp.end()){
                ret.push_back(mp[target - nums[i]]);
                ret.push_back(i);
                return ret;
            }
            mp[nums[i]] = i;

        }
        return vector<int> ();
    }
};
